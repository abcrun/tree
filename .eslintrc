{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "standard",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "globals": {
    "Env": "readonly"
  },
  "rules": {
    "prettier/prettier": "warn",
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "global-require": "off",
    "no-console": "off"
  }
}
